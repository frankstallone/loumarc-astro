---
import Head from "../components/Head.astro";
export interface Props {
  content: {
    title?: string;
    description?: string;
  };
}

const { content } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
  <Head title={content.title} description={content.description} />
  <body>
    <slot />
  </body>
</html>
