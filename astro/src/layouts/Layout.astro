---
import Head from '../components/Head.astro';
import Footer from '../components/Footer.astro';
import type { Props } from '../components/Head.astro';

const { meta, productSchema, overlay } = Astro.props;
---

<!DOCTYPE html>
<html lang="en">
  <Head meta={meta} productSchema={productSchema} />
  <body>
    <slot />
    <Footer overlay={overlay ? true : false} />
  </body>
</html>
